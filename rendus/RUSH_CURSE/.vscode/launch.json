	"version": "0.2.0",
	"configurations": [
		{
			"name": "alcu DEBUG",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/alcu.debug",
			"args": [],
			"cwd": "${workspaceFolder}",
			"externalConsole": false,
			"stopAtEntry": false,
			"environment": [],
			"MIMode": "gdb",
			"miDebuggerPath": "/usr/bin/gdb",
			"preLaunchTask": "DEBUG build",
			"setupCommands": [
				{"text": "-gdb-set follow-fork-mode parent"}
			]
		}
	]
}
